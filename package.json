{
  "name": "react-latex-modern",
  "version": "4.0.0",
  "description": "Modern React 19 compatible LaTeX rendering for React apps",
  "author": "Jarett <jarett@arbutuscode.ca>",
  "contributors": [
    "Harun Urhan"
  ],
  "homepage": "https://github.com/jarettrude/react-latex-modern",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jarettrude/react-latex-modern.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "keywords": [
    "react",
    "katex",
    "latex",
    "math"
  ],
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "scripts": {
    "build": "npm run build:types && rollup -c",
    "build:types": "tsc --emitDeclarationOnly --project tsconfig.json",
    "dev": "rollup -c -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "katex": "^0.16.25"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.0.2",
    "@types/katex": "^0.16.7",
    "@vitejs/plugin-react": "^5.1.0",
    "esbuild": "^0.25.12",
    "jsdom": "^27.1.0",
    "rollup": "^4.52.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "files": [
    "dist"
  ]
}
